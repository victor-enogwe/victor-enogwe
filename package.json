{
    "name": "enogwe",
    "version": "0.0.0-development",
    "description": "Wordpress theme for victorenogwe.me",
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "build": "export NODE_ENV=production && webpack",
        "dev": "webpack -w",
        "test:php": "vendor/bin/phpunit --bootstrap vendor/autoload.php --coverage-lcov coverage/php --coverage-html coverage/php --testdox tests/php",
        "test:js": "jest",
        "test": "echo \"Error: no test specified\" && exit 1",
        "commit": "git cz",
        "lint": "tslint --project .",
        "lint:fix": "tslint --fix --project .",
        "semantic-release": "semantic-release"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/victor-enogwe/enogwe.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/victor-enogwe/enogwe/issues"
    },
    "homepage": "https://github.com/victor-enogwe/enogwe#readme",
    "release": {
        "branch": "master",
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            [
                "@semantic-release/changelog",
                {
                    "changelogFile": "CHANGELOG.md"
                }
            ],
            [
                "@semantic-release/github",
                {
                    "assets": "theme/**"
                }
            ]
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "jest": {
        "preset": "ts-jest",
        "clearMocks": true,
        "coverageDirectory": "coverage/js",
        "collectCoverage": true,
        "testEnvironment": "jest-environment-jsdom",
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "setupFiles": [
            "./tests/setup.ts"
        ],
        "roots": [
            "ts/",
            "tests/"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx",
            "ts",
            "tsx"
        ],
        "coverageReporters": [
            "lcov"
        ]
    },
    "dependencies": {
        "@types/react": "16.8.19",
        "@types/react-dom": "16.8.4",
        "@types/superagent": "4.1.1",
        "@wordpress/block-editor": "2.0.3",
        "@wordpress/components": "7.4.0",
        "@wordpress/data": "4.5.0",
        "@wordpress/editor": "9.2.6",
        "@wordpress/element": "2.4.0",
        "@wordpress/i18n": "3.4.0",
        "bootstrap": "4.3.1",
        "li": "1.3.0",
        "react": "16.8.6",
        "react-dom": "16.8.6",
        "reactstrap": "8.0.0",
        "superagent": "5.0.5",
        "wpapi": "2.0.0-alpha.1"
    },
    "devDependencies": {
        "@semantic-release/changelog": "3.0.2",
        "@semantic-release/commit-analyzer": "6.1.0",
        "@semantic-release/github": "5.2.10",
        "@semantic-release/release-notes-generator": "7.1.4",
        "@types/browser-sync-webpack-plugin": "^2.2.0",
        "@types/classnames": "2.2.8",
        "@types/enzyme": "3.9.3",
        "@types/enzyme-adapter-react-16": "1.0.5",
        "@types/extract-text-webpack-plugin": "^3.0.4",
        "@types/jest": "24.0.13",
        "@types/node": "12.0.4",
        "@types/reactstrap": "8.0.1",
        "@types/terser-webpack-plugin": "^2.2.0",
        "@types/webpack": "4.4.32",
        "@types/wpapi": "1.1.0",
        "awesome-typescript-loader": "5.2.1",
        "browser-sync": "2.26.7",
        "browser-sync-webpack-plugin": "2.2.2",
        "commitizen": "^4.0.3",
        "css-loader": "2.1.1",
        "cz-conventional-changelog": "2.1.0",
        "enzyme": "3.10.0",
        "enzyme-adapter-react-16": "1.14.0",
        "enzyme-to-json": "3.3.5",
        "extract-text-webpack-plugin": "4.0.0-beta.0",
        "fast-glob": "2.2.7",
        "jest": "24.8.0",
        "node-sass": "4.12.0",
        "sass-loader": "7.1.0",
        "semantic-release": "^15.13.31",
        "source-map-loader": "0.2.4",
        "terser-webpack-plugin": "1.3.0",
        "ts-jest": "24.0.2",
        "ts-node": "^8.5.2",
        "tslint": "5.16.0",
        "tslint-config-standard": "8.0.1",
        "tslint-etc": "1.5.3",
        "tslint-react": "4.0.0",
        "typescript": "3.4.5",
        "webpack": "4.32.2",
        "webpack-cli": "3.3.2"
    }
}
